h2#site-name 
  a(href="/") Adil's Portfolio

  if session && session.loggedIn
    a(href="/user/logout")
      button(type="button") Logout

    if session.isAdmin
      a(href="/admin/skills")
        button.manage-btn(type="button") Manage Skills
      a(href="/admin/projects")
        button.manage-btn(type="button") Manage Projects
  else
    a(href="/user/login")
      button(type="button") Login
